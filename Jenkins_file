pipeline {
    agent any
environment {
        GITHUB_REPO = 'https://github.com/your-username/your-repo.git'
        BUILD_DIR = 'build'
    }

    stages {
        stage('Checkout') {
            steps {
                echo 'Cloning repository...'
                git branch: 'main', url: "${GITHUB_REPO}"
            }
        }
}
}


   


   
