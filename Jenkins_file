pipeline {
    agent any
environment {
        GITHUB_REPO = 'https://github.com/Hadakar-Kasturi/Ekart_mine.git'
    }

    stages {
        stage('Checkout') {
            steps {
                echo 'Cloning repository...'
                git branch: 'main', url: "${GITHUB_REPO}"
            }
        }
}
}


   


   
